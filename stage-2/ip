decl
int a[2][2],sum,b[2][2],c[2][2],i,j,k,x,y,z;
enddecl
begin
read(x);
read(y);
read(z);
i=0;
while(i<x)do
j=0;
while(j<y)do
read(a[i][j]);
j=j+1;
endwhile;
i=i+1;
endwhile;
i=0;
while(i<y)do
j=0;
while(j<z)do
read(b[i][j]);
j=j+1;
endwhile;
i=i+1;
endwhile;
i=0;
while(i<x)do
j=0;
while(j<z)do
k=0;
while(k<y)do
c[i][j]=c[i][j]+(a[i][k]*b[k][j]);
k=k+1;
endwhile;
j=j+1;
endwhile;
i=i+1;
endwhile;
i=0;
while(i<x)do
j=0;
while(j<z)do
write(c[i][j]);
j=j+1;
endwhile;
i=i+1;
endwhile;
end;
