decl
int a[2][2],sum,b[2][2],c[2][2],i,j,k,x,y;
enddecl
begin
i=0;
j=0;
k=0;
while(i<2)do
j=0;
while(j<2)do
read(a[i][j]);
j=j+1;
endwhile;
i=i+1;
endwhile;
x=0;
y=0;
while(x<2)do
y=0;
while(y<2)do
read(a[x][y]);
y=y+1;
endwhile;
x=x+1;
endwhile;
i=0;
while(i<2)do
j=0;
while(j<2)do
k=0;
sum=0;
while(k<2)do
sum=sum+a[i][k]*b[k][j];
k=k+1;
endwhile;
c[i][j]=sum;
write(c[i][j]);
j=j+1;
endwhile;
i=i+1;
endwhile;
end;
